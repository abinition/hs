<func id="get">
  <brief>
    Get data from stdin.
  </brief>
  <args>
      <arg id="prompt" type="str">
         <desc>
         the prompt (echoed to stdout).
         </desc>
      </arg>
  </args>
  <lvalue id="result" type="list">
      <ret id="list" status="$ACKNOWLEDGE">
         <desc>
         A list of values, appropriately 
         typecast,  read from stdin.  
         Values are delimited 
         by white space characters
         </desc>
      </ret>
    <ret id="NULLString" status="%EOF">
      <desc>
      At the end of file, the result is "".
      </desc>
    </ret>
  </lvalue>
  <body ref="get.htm" />
  <exceptions />
  <examples>
    <example>
      get.hyp
    </example>
  </examples>
  <links />
</func>

