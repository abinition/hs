<func id="node_prevlastcousin">
   <brief>Returns the previous last cousin of a node.</brief>

   <args>
      <arg id="node" type="list">
         <desc>A list variable.</desc>
      </arg>
   </args>

   <lvalue id="result" type="list">
      <ret id="element" status="$ACKNOWLEDGE">
         <desc>Returns the last child node of the argument's parent's previous sibling or an empty string if no previous cousin node is present. Nodes are of type list.</desc>
      </ret>
      <ret id="empty string" status="%INVALID">
         <desc>If the node is a root node, then it cannot have a cousin, and an empty string is returned.</desc>
      </ret>
   </lvalue>

   <body />
   
   <examples>
    <example>node_prevlastcousin.hyp</example>
   </examples>

   <links>
      <link>node_name</link>
      <link>node_root</link>
      <link>node_parent</link>
      <link>node_firstchild</link>
      <link>node_lastchild</link>
      <link>node_nextsibling</link>
      <link>node_prevsibling</link>
      <link>node_nextfirstcousin</link>
      <link>node_nextfirstcousin</link>
      <link>node_getnodebyattr</link>
   </links>
</func>

