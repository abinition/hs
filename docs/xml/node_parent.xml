<func id="node_parent">
   <brief>Returns the parent of a node.</brief>

   <args>
      <arg id="node" type="list">
         <desc>A list or string variable.</desc>
      </arg>
   </args>

   <lvalue id="result" type="list">
      <ret id="element" status="$ACKNOWLEDGE">
         <desc>Returns the parent node of the argument.</desc>
      </ret>
      <ret id="empty string" status="%INVALID">
         <desc>If the argument is a root node, then it has no parent, and an empty string is returned.</desc>
      </ret>
   </lvalue>

   <body />

   <links>
      <link>node_name</link>
      <link>node_root</link>
      <link>node_parent</link>
      <link>node_firstchild</link>
      <link>node_lastchild</link>
      <link>node_nextsibling</link>
      <link>node_prevsibling</link>
      <link>node_nextfirstcousin</link>
      <link>node_nextfirstcousin</link>
      <link>node_getnodebyattr</link>
   </links>
</func>

