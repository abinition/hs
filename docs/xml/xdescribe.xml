<func id="xdescribe">
  <brief>
    Describe as XML a variable.
  </brief>
  <args>
    <arg id="variable" type="list">
      <desc>
         A variable or value of any type.
      </desc>
    </arg>
  </args>
  <lvalue id="status" type="str">
    <ret id="$ACKNOWLEDGE" status="$ACKNOWLEDGE">
      <desc>
         The describe function was successful.
      </desc>
    </ret>
  </lvalue>
  <body ref="describe.htm" />
  <exceptions />
  <examples>
    <example>
      describe.hyp
    </example>
  </examples>
  <links>
     <link>describe</link>
     <link>fdescribe</link>
     <link>sdescribe</link>
     <link>xfdescribe</link>
     <link>xsdescribe</link>
  </links>
</func>

